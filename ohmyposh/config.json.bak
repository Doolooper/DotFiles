{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "black": "#14171d",
    "blue": "#8ba4b0",
    "bright-black": "#5C6066",
    "bright-blue": "#7fb4ca",
    "bright-cyan": "#7aa89f",
    "bright-green": "#87a987",
    "bright-magenta": "#938aa9",
    "bright-red": "#e46876",
    "bright-white": "#c5c9c7",
    "bright-yellow": "#e6c384",
    "cyan": "#8ea4a2",
    "diag-error": "#e46876",
    "diag-hint": "#7aa89f",
    "diag-info": "#8ba4b0",
    "diag-ok": "#87a987",
    "diag-warning": "#e6c384",
    "git-added": "#87a987",
    "git-changed": "#e6c384",
    "git-removed": "#e46876",
    "green": "#8a9a7b",
    "magenta": "#a292a3",
    "none": "transparent",
    "red": "#c4746e",
    "special": "#8ea4a2",
    "white": "#c5c9c7",
    "yellow": "#e6c384"
  },
  "valid_line": {
    "template": "❯ ",
    "foreground": "p:diag-ok",
    "background": "p:none"
  },
  "secondary_prompt": {
    "template": "󰇘  ",
    "foreground": "p:bright-black",
    "background": "p:none"
  },
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#ffffff",
    "template": "❯ "
  },
  "error_line": {
    "template": "❯ ",
    "foreground": "p:diag-error",
    "background": "p:none"
  },
  "pwd": "osc7",
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "overflow": "hide",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "folder_separator_icon": "/",
            "home_icon": "~",
            "max_depth": 2,
            "mixed_threshold": 5,
            "style": "agnoster_full"
          },
          "foreground": "p:blue",
          "background": "p:none",
          "type": "path",
          "style": "plain",
          "templates": [
            "{{ if not .Writable }} <p:yellow>{{ else }}{{ end }}",
            "󰉋 {{ path .Path .Location }}</>"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "overflow": "hide",
      "newline": true,
      "segments": [
        {
          "properties": {
            "branch_icon": "󰊢 ",
            "cache_duration": "none",
            "fetch_stash_count": true,
            "fetch_status": true,
            "status_formats": {
              "Added": "<p:git-added>+%d</>",
              "Deleted": "<p:git-removed>-%d</>",
              "Modified": "<p:git-changed>~%d</>",
              "Unmerged": "<p:diag-info>X%d</>",
              "Untracked": "<p:diag-warning>?%d</>"
            }
          },
          "type": "git",
          "style": "plain",
          "templates": [
            "<p:special>{{ .HEAD }}</>",
            "{{ if or (gt .Ahead 0) (gt .Behind 0) }} <p:bright-cyan>{{ .BranchStatus }}</>{{ end }}",
            "{{ if .Working.Changed }} {{ .Working.String }}{{ end }}",
            "{{ if or (and .Working.Changed .Staging.Changed) (gt .StashCount 0) }}|{{ end }}",
            "{{ if (gt .Staging.Added 0) }}{{ .Staging.String }}{{ end }}",
            "{{ if gt .StashCount 0 }}<p:blue>*{{ .StashCount }}</>{{ end }}"
          ]
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "background": "p:none",
          "type": "project",
          "style": "plain",
          "foreground_templates": [
            "{{ if .Type }}",
            "{{ if eq .Type \"node\" }}#5c8763{{ end }}",
            "{{ if eq .Type \"cargo\" }}#b74f0e{{ end }}",
            "{{ if eq .Type \"dotnet\" }}#957FB8{{ end }}",
            "{{ if eq .Type \"powershell\" }}#0276bc{{ end }}",
            "{{ else }}p:fg-dim{{ end }}"
          ],
          "templates": [
            "{{ if .Type }}",
            "{{ if eq .Type \"node\" }} 󰎙{{ end }}",
            "{{ if eq .Type \"cargo\" }} 󱘗{{ end }}",
            "{{ if eq .Type \"dotnet\" }} {{ end }}",
            "{{ if eq .Type \"powershell\" }} 󰨊{{ end }}",
            "{{ end }}",
            "{{ if .Version }} {{ .Version }}{{ if .Name }} {{ .Name }}{{ end }}{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none",
            "style": "austin",
            "threshold": 500
          },
          "template": " {{ .FormattedMs }}",
          "foreground": "p:bright-black",
          "type": "executiontime",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none",
            "status_separator": " | "
          },
          "template": "❯",
          "type": "status",
          "style": "plain",
          "foreground_templates": [
            "{{ if (gt .Code 0) }}p:diag-error{{ else }}p:diag-ok{{ end }}"
          ]
        }
      ]
    }
  ],
  "version": 3,
  "shell_integration": true,
  "final_space": true
}
